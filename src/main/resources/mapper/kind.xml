<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easybusan.repository.interfaces.KindRepository">
    <select id="readKindById" resultType="com.easybusan.repository.model.Kind">
        select *
        from kind
        where kind_id = #{id}
    </select>

</mapper>